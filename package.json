{
  "name": "server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development nodemon ./bin/www",
    "format": "find -name '*.js' -not -path './node_modules/*' | xargs prettier --write",
    "lint": "./node_modules/.bin/eslint .",
    "test": "LOG_LEVEL=error NODE_ENV=test mocha --recursive --exit tests/",
    "clean": "npm run format && npm run lint",
    "resetDatabase": "sequelize db:drop; sequelize db:create && sequelize db:migrate && sequelize db:seed:all --debug",
    "resetTestDatabase": "NODE_ENV=test sequelize db:drop; NODE_ENV=test sequelize db:create && NODE_ENV=test sequelize db:migrate"
  },
  "dependencies": {
    "app-root-path": "^2.1.0",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "jsonwebtoken": "^8.4.0",
    "morgan": "~1.9.0",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^7.8.0",
    "pg-hstore": "^2.3.2",
    "sequelize": "^4.42.0",
    "sequelize-cli": "^5.4.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "eslint": "^5.13.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^2.7.0",
    "eslint-plugin-react": "^7.12.4",
    "mocha": "^5.2.0",
    "nodemon": "^1.18.10",
    "prettier": "^1.16.4",
    "supertest": "^3.4.2"
  }
}
